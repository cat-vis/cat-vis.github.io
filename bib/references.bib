  url = {https://cran.r-project.org/web/packages/waffle/waffle.pdf}
}

@inproceedings{siirtola2014proportions,
  title={Bars, pies, doughnuts \& tables--Visualization of proportions},
  author={Siirtola, Harri},
  abstract = {Visualization of proportions is one of the most common visualization types encountered in the media.
Stacked bar charts, doughnut and pie charts – the most common visualizations of proportions – have all
keen advocates and critics, and there are conflicting results about their performance. This study presents
an experiment with a basic but ecologically valid task to evaluate the performance of these techniques. The
result shows that the stacked bar chart is superior to doughnut and pie charts in task performance. However,
75% of the participants regarded pie charts as the most pleasing or second-pleasing to use, and almost half
of the participants (44%) perceived the pie or doughnut chart to be the fastest visualization to understand.
Only six participants recognized the bar chart as the fastest technique, and half of them still preferred the
pie chart over them. This subjective preference at the expense of performance may explain why pie charts
are so widely used in spite of being criticized.},
  keywords = {contribution:evaluation, family:size-encoding, alignment:radial, alignment:linear, dimensionality:univariate, data_type:homogeneous, cardinality:low},
  booktitle={Proceedings of the 28th International BCS Human Computer Interaction Conference (HCI 2014) 28},
  series = {BCS HCI},
  doi = {10.14236/ewic/hci2014.30},
  publisher = {BCS},
  pages={240--245},
  year={2014},
  url = {https://www.scienceopen.com/document_file/513c6c7d-e117-48a7-ac72-e738c3ca964e/ScienceOpen/240_Siirtola.pdf}
}

@inproceedings{stasko2000sunburst,
  title={Focus + context display and navigation techniques for enhancing radial, space-filling hierarchy visualizations},
  author={Stasko, John and Zhang, Eugene},
  keywords = {contribution:technique, family:space-filling, alignment:radial, data_type:homogeneous, dimensionality:multivariate, cardinality:low},
  abstract={Sunburst diagram (aka ring chart, multi-level pie chart, nested pie chart), the radial equivalent of an icicle plot. ABSTRACT: Radial, space-filling visualizations can be useful for depicting
information hierarchies, but they suffer from one
major problem. As the hierarchy grows in size, many items
become small, peripheral slices that are difficult to distinguish.
We have developed three visualization/interaction
techniques that provide flexible browsing of the display. The
techniques allow viewers to examine the small items in detail
while providing context within the entire information
hierarchy. Additionally, smooth transitions between views
help users maintain orientation within the complete information
space.},
  booktitle={IEEE Symposium on Information Visualization 2000. INFOVIS 2000. Proceedings},
  pages={57--65},
  year={2000},
  doi={10.1109/INFVIS.2000},
  series = {InfoVis Symposium},
  url={https://faculty.cc.gatech.edu/~john.stasko/papers/infovis00.pdf},
  organization={IEEE},
  publisher={IEEE}
}

