@article{rosario2004mapping,
  title={Mapping nominal values to numbers for effective visualization},
  author={Rosario, Geraldine E and Rundensteiner, Elke A and Brown, David C and Ward, Matthew O and Huang, Shiping},
  journal={Information Visualization},
  volume={3},
  number={2},
  pages={80--95},
  year={2004},
  publisher={SAGE Publications Sage UK: London, England},
  series={SAGE Information Visualization},
  doi={10.1057/palgrave.ivs.9500072},
  url={https://journals.sagepub.com/doi/pdf/10.1057/palgrave.ivs.9500072},
  keywords={contribution:technique, family:projection, data_type:homogeneous, dimensionality:multivariate, cardinality:moderate},
  abstract={Similar to MCA. Similar categories are positioned close to each other. ABSTRACT: Data sets with a large numbers of nominal variables, including some with large
number of distinct values, are becoming increasingly common and need to be
explored. Unfortunately, most existing visual exploration tools are designed to
handle numeric variables only. When importing data sets with nominal values
into such visualization tools, most solutions to date are rather simplistic. Often,
techniques that map nominal values to numbers do not assign order or spacing
among the values in a manner that conveys semantic relationships. Moreover,
displays designed for nominal variables usually cannot handle high cardinality
variables well. This paper addresses the problem of how to display nominal
variables in general-purpose visual exploration tools designed for numeric
variables. Specifically, we investigate (1) how to assign order and spacing
among the nominal values, and (2) how to reduce the number of distinct
values to display. We propose a new technique, called the Distance-
Quantification-Classing (DQC) approach, to preprocess nominal variables before
being imported into a visual exploration tool. In the Distance Step, we identify
a set of independent dimensions that can be used to calculate the distance
between nominal values. In the Quantification Step, we use the independent
dimensions and the distance information to assign order and spacing among
the nominal values. In the Classing Step, we use results from the previous steps
to determine which values within the domain of a variable are similar to each
other and thus can be grouped together. Each step in the DQC approach can
be accomplished by a variety of techniques. We extended the XmdvTool
package to incorporate this approach. We evaluated our approach on several
data sets using a variety of measures.}
}

@article{tenenhaus1985analysis,
  title={An analysis and synthesis of multiple correspondence analysis, optimal scaling, dual scaling, homogeneity analysis and other methods for quantifying categorical multivariate data},
  author={Tenenhaus, Michel and Young, Forrest W},
  journal={Psychometrika},
  volume={50},
  pages={91--119},
  year={1985},
  publisher={Springer},
  series={Other},
  doi={https://doi.org/10.1007/BF022941510},
  abstract={Several different methods for quantification, including Multiple Correspondence Analysis (MCA), Optimal Scaling/Scoring, Homogeneity Analysis and Scalogram Analysis, all lead to the same equation and can thus be considered the same technique. They show pairwise relations for multiple variables and are not suitable for a large number of categories. Can often be improved using adjusted inertia. See also Greenacre and Blasius, 2006; D’Enza and Greenacre 2012. ABSTRACT: We discuss a variety of methods for quantifying categorical multivariate data. These methods have been proposed in many different countries, by many different authors, under many different names. In the first major section of the paper we analyze the many different methods and show that they all lead to the same equations for analyzing the same data. In the second major section of the paper we introduce the notion of a duality diagram, and use this diagram to synthesize the many superficially different methods into a single method.},
  keywords={contribution:technique, family:projection, data_type:homogeneous, dimensionality:multivariate, cardinality:moderate}
}

@inproceedings{johansson2008quant,
  title={Interactive quantification of categorical variables in mixed data sets},
  author={Johansson Fernstad, Sara and Jern, Mikael and Johansson, Jimmy},
  booktitle={2008 12th International Conference Information Visualisation},
  pages={3--10},
  year={2008},
  organization={IEEE},
  series={IV},
  abstract={Data sets containing a combination of categorical and
continuous variables (mixed data sets) are difficult to analyse
since no generalized similarity measure exists for categorical
variables. Quantification of categorical variables
makes it possible to represent this type of data using techniques
designed for numerical data. This paper presents
a quantification process of categorical variables in mixed
data sets that incorporates information on relationships
among the continuous variables into the process, as well
as utilizing the domain knowledge of a user. An interactive
visualization environment using parallel coordinates as a
visual interface is provided, where the user is able to control
the quantification process and analyse the result. The
efficiency of the approach is demonstrated using two mixed
data sets.},
  doi = {10.1109/IV.2008.33},
  url={https://www.researchgate.net/profile/Mikael-Jern/publication/4358668_Interactive_Quantification_of_Categorical_Variables_in_Mixed_Data_Sets/links/00b7d51e559e1e9d1e000000/Interactive-Quantification-of-Categorical-Variables-in-Mixed-Data-Sets.pdf},
  keywords={contribution:technique, family:projection, data_type:heterogeneous, dimensionality:multivariate, cardinality:high}
}

