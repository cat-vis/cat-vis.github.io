@article{allen2002trilinear,
  title={Using and Interpreting the Trilinear Plot},
  author={Allen, Terry},
  abstract = {Trilinear plot (see also Snee, 1974). Supports one variable with three mutually exclusive categories. Statistical graphics are powerful tools for analyzing research and policy data. They have been used extensively for centuries to gain meaningful insights into data structures that would otherwise be difficult to discern. Unfortunately, many investigators underutilize graphical procedures when they examine data. This article proposes a visual approach to empirical investigation by demonstrating the ease with which graphics can make hidden patterns jump off the page. More specifically, it encourages the use of the trilinear plot, an effective analytic tool first introduced to Chance readers by Wainer (1995). The trilinear plot has been used successfully in political science, chemistry, and geology. Examples presented here show that it could also benefit policy makers, industry statisticians, and researchers in other fields.},
  keywords = {contribution:technique, family:other, data_type:homogeneous, dimensionality:univariate, cardinality:low, alignment:linear},
