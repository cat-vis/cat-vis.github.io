  series={Book},
  abstract={Visualisations for CA, including CA Maps and CA Biplots.},
  doi={https://doi.org/10.1201/9781315369983},
  keywords={contribution:technique,family:projection,data_type:homogeneous,dimensionality:bivariate, cardinality:moderate}
