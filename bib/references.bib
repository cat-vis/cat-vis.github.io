@article{wickham2011product,
  title={Product plots},
  author={Wickham, Hadley and Hofmann, Heike},
  journal={IEEE Transactions on Visualization and Computer Graphics},
  volume={17},
  number={12},
  pages={2223--2230},
  year={2011},
